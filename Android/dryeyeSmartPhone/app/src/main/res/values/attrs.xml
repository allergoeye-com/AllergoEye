<resources>

    <!-- Declare custom theme attributes that allow changing which styles are
         used for button bars depending on the API level.
         ?android:attr/buttonBarStyle is new as of API 11 so this is
         necessary to support previous API levels. -->
    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>
    <declare-styleable name="AnimatedLayout">
        <attr name="drag_orient" format="string" />
        <attr name="size_hand" format="dimension" />
        <attr name="idToDrag" format="reference" />
        <attr name="isVisible" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CMenuTogleButton">
        <attr name="icon_pushed" format="boolean" />
        <attr name="eneble_togle_color" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CMenuButton">
        <attr name="disable" format="boolean" />
        <attr name="color_up" format="color" />
        <attr name="color_down" format="color" />
        <attr name="color_disable" format="color" />
        <attr name="disable_colors" format="boolean" />
        <attr name="colors_png" format="boolean" />
        <attr name="icon_push" format="reference|color" />
        <attr name="icon_pop" format="reference|color" />
        <attr name="icon_disable" format="reference|color" />
        <attr name="long_click" format="boolean"/>

    </declare-styleable>
    <declare-styleable name="LayoutButton">
        <attr name="childs_color_up" format="color" />
        <attr name="childs_color_down" format="color" />
        <attr name="disable_change_color" format="boolean" />
    </declare-styleable>
    <declare-styleable name="EditableSpinner">
        <attr name="idLayout" format="reference" />
        <attr name="idEdit" format="reference" />
        <attr name="idText" format="reference" />
    </declare-styleable>
    <declare-styleable name="CEdit">
        <attr name="border_draw" format="boolean" />
        <attr name="border_color" format="color" />
        <attr name="border_focus_color" format="color" />
        <attr name="border_width" format="integer" />
        <attr name="border_radius" format="integer" />
        <attr name="show_ui" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ProgressView">
        <attr name="hasThumb" format="boolean" />
    </declare-styleable>

    <declare-styleable name="LinearLayoutShadow">
        <attr name="shadow_color" format="color" />
    </declare-styleable>

    <declare-styleable name="LinearLayoutBorder">
        <attr name="color_border" format="color" />
        <attr name="cell_draw" format="boolean" />
    </declare-styleable>
    <declare-styleable name="HandLayout">
        <attr name="idToHandButton" format="reference" />
    </declare-styleable>
    <declare-styleable name="CComboBox">
        <attr name="text_color" format="color" />
        <attr name="bg_color" format="color" />
        <attr name="text_size" format="integer" />
    </declare-styleable>
    <declare-styleable name="CSpinButton">
        <attr name="is_spin_mode" format="boolean" />

    </declare-styleable>
    <declare-styleable name="ImageSlider">
        <attr name="view_title" format="boolean" />
        <attr name="view_list" format="boolean" />

    </declare-styleable>


</resources>
